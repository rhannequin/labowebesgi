<p id="notice"><%= notice %></p>

<p>
  <strong><%= t '.label_title' %> :</strong>
  <%= @event.title %>
</p>

<p>
  <strong><%= t '.label_starts_at' %> :</strong>
  <%= l @event.starts_at, format: :long %>
</p>

<%= link_to events_path do %>
  <button class="btn btn-primary"><%= t(:back) %></button>
<% end %>
